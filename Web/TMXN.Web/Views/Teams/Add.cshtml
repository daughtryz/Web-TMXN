@model TMXN.Common.InputModels.TeamInputModel


    <form asp-controller="Teams" asp-action="Add">
        <div class="form-group">
            <label for="name">Team Name:</label>
            <input asp-for="Name" type="name" class="form-control" id="name">
        </div>
        <div class="form-group">
            <label for="logo">Logo:</label>
            <input asp-for="Logo" type="logo" class="form-control" id="logo">
        </div>
        <div class="form-group">
            <label for="tag">Tag:</label>
            <input asp-for="Tag" type="tag" class="form-control" id="tag">
        </div>
        <button type="submit" class="btn btn-primary">Create!</button>

    </form>

